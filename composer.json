{
    "name": "wenhainan/lavarel-calendar",
    "description": "Laravel Livewire calendar component",
    "keywords": [
        "wenhainan",
        "lavarel-calendar"
    ],
    "homepage": "https://github.com/wenhainan/lavarel-calendar",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "wenhainan",
            "email": "whndeweilai@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "*",
        "livewire/livewire": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Wenhainan\\LavarelCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wenhainan\\LavarelCalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wenhainan\\LavarelCalendar\\LavarelCalendarServiceProvider"
            ],
            "aliases": {
                "LavarelCalendar": "Wenhainan\\LavarelCalendar\\LavarelCalendarFacade"
            }
        }
    }
}
